= render partial: 'search/search_box'

%p= "About #{@response['TotalResults']} results for #{@query_parameter}"

%ul
  - @results.each do |result|
    %li
      %p= link_to(result['Title']['Text'].html_safe, result['Links'][0]['Uri'])
      %p= result['Links'][0]['Uri']
      %p= result['Summary']['Text'].html_safe

